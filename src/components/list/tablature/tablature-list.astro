---
import type { Tablature } from "../../../clients/model";
import TablatureListItem from "./tablature-list-item.astro";

type Props = {
    tablatures: Tablature[];
};

const { tablatures } = Astro.props;
---

<ul class="list bg-base-100 rounded-box shadow-md">
    {tablatures.map((tablature) => <TablatureListItem tablature={tablature} />)}
</ul>
